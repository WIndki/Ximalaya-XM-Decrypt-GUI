{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../dist/electron",
    "module": "CommonJS",
    "target": "ES2020",
    "lib": ["ES2020"],
    "skipLibCheck": true,
    "noEmit": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "*.ts",
    "services/*.ts"
  ]
}
